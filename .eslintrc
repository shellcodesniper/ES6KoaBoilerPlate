{
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "env": {
    "browser": false,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "no-console": 0
  }
}
